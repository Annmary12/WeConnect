{
   "name": "weconnect",
   "version": "1.0.0",
   "description": "",
   "main": "app.js",
   "scripts": {
      "start": "babel-node app.js",
      "start-client": "webpack-dev-server --hot",
      "start-dev": "nyc nodemon --watch server --exec babel-node app.js",
      "lint": "eslint ./ || true",
      "test-migrate": "sequelize db:migrate:undo:all && sequelize db:migrate",
      "test": "set  NODE_ENV=test&& npm run test-migrate&&  nyc mocha ./server/test --exit  --timeout 7000 --compilers js:babel-register --exit",
      "migrate": "sequelize db:migrate",
      "heroku-postbuild": "npm run migrate",
      "coverage": "nyc report --reporter=text-lcov | coveralls",
      "report-coverage": "cat ./coverage/lcov.info | coveralls",
      "check-coverage": "nyc check-coverage --lines 70 --per-file",
      "coveralls": "nyc --reporter=lcov --reporter=text-lcov npm test"
   },
   "devDependencies": {
      "babel": "^6.23.0",
      "babel-eslint": "^8.2.2",
